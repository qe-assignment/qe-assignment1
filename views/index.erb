<html>
    <head>
        <title>QE Assignment 1</title>
        <link rel="stylesheet" type="text/css" href="style.css">
    </head>
    <body>

        <div>
            <button id="start_button" onclick="start_listening()" type="button">Start listening</button>
        </div>

        <div>
            <form id="topic_form">
                Filter parameter<br>
                <input name="topic_text_field" type="text" id="topic" value="fashion">
                <input name="topic_button" type="submit" value="Change topic">
            </form>
        </div>

        <div>
            <img id="newest_image"/>
            <p id="running_average"> Running average: </p>
            <p id="male_count"> Male count: </p>
            <p id="female_count"> Female count: </p>
        </div>

        <div id="thumbnails">
            <% 10.times do |i| %>
                <img/>
            <% end %>
        </div>

        <script src="https://code.jquery.com/jquery-2.1.3.js"></script>
        <script src="script.js"></script>

    </body>
</html>
